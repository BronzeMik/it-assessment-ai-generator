{
    "version": 2,
    "builds": [
      {
        "src": "api/generate-assessment.js",
        "use": "@vercel/node" 
      }
    ],
    "functions": {
    "api/*.js": {
      "maxDuration": 60
    }
  },
    "routes": [
      {
        "src": "/api/generate-assessment",
        "dest": "api/generate-assessment.js",
        "methods": ["POST", "OPTIONS"]
      }
    ]
  }
